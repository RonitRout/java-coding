package java_coding_string;

import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class FirstNonReaptingChar {

	public static void main(String[] args) {
		String str="Fuck u ";
		Map<Character , Long> count= str.toLowerCase().chars().filter(ch->ch!=' ').mapToObj(c -> (char) c).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
				}

}
